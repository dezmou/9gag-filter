{
    "name": "9gag-filter",
    "version": "0.0.1",
    "description": "Burry the shitposts",
    "manifest_version": 2,
    "permissions": [
        "storage",
        "<all_urls>"
    ],
    "content_scripts": [
        {
            "matches": [
                "*://9gag.com/*"
            ],
            "all_frames": false,
            "js": [
                "index.js"
            ]
        }
    ],
    "background": {
        "scripts": [
            "background.js"
        ]
    },
    "icons": {
        "16": "icon16.png",
        "32": "icon32.png",
        "48": "icon48.png",
        "128": "icon128.png"
    }
}